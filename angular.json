{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "yarn"
  },
  "newProjectRoot": "projects",
  "projects": {
    "profile-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": false,
          "inlineStyle": true,
          "style": "scss",
          "skipTests": true,
          "prefix": "gcs"
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/profile-app",
      "sourceRoot": "projects/profile-app/src",
      "prefix": "gcs",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "webpack.config.js"
            },
            "outputPath": "dist/profile-app",
            "index": "projects/profile-app/src/index.html",
            "main": "projects/profile-app/src/main.ts",
            "polyfills": "projects/profile-app/src/polyfills.ts",
            "tsConfig": "projects/profile-app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/profile-app/src/favicon.ico",
              "projects/profile-app/src/assets"
            ],
            "styles": [
              "projects/profile-app/src/css/styles.scss",
              "node_modules/font-awesome/css/font-awesome.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/profile-app/src/environments/environment.ts",
                  "with": "projects/profile-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "profile-app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "profile-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "profile-app:build"
          }
        }
      }
    }
  },
  "defaultProject": "profile-app"
}
